{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "GOOGLE_MAPS_API_KEY": "@google-maps-api-key",
    "GOOGLE_CLOUD_VISION_CREDENTIALS": "@google-cloud-vision-credentials",
    "NODE_ENV": "production"
  }
} 